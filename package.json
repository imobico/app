{
  "name": "imoblr-app",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "native": "cd apps/expo && bun start",
    "watch": "bun workspaces foreach -pi run watch",
    "vercel:install": "bun set version 3.5 && bun install",
    "web": "turbo dev --parallel --filter={next-app,@t4/api}",
    "web:extract": "DISABLE_EXTRACTION=false bun workspace next-app dev",
    "web:prod": "bun workspace next-app build",
    "web:prod:serve": "bun workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "bun build",
    "build": "bun workspaces foreach --all --exclude next-app run build",
    "upgrade:tamagui": "bun up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "bun up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency ."
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "check-dependency-version-consistency": "^3.0.3",
    "eslint": "^8.21.0",
    "node-gyp": "^9.3.1",
    "prettier": "^2.7.1",
    "turbo": "latest",
    "typescript": "^5.3.3"
  },
  "packageManager": "bun@4.1.0"
}
