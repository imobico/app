{
  "expo": {
    "name": "imoblr",
    "slug": "imoblr",
    "scheme": "imoblr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false,
      "url": "https://u.expo.dev/dce7358a-68f2-4690-941b-169757e68b17"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imoblr.app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.imoblr.app"
    },
    "extra": {
      "eas": {
        "projectId": "85fc6ccd-0ce1-4e4d-804c-b15df989f97e"
      }
    },
    "owner": "timothymiller",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      {
        "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
      }
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  }
}
